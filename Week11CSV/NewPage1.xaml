<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Week11CSV.NewPage1"
             Title="NewPage1">
    <ContentPage.Resources>
        <ResourceDictionary>
            <Style TargetType="Label">
                <Setter Property="Padding" Value="15"/>
                <Setter Property="FontSize" Value="Medium" />
                <Setter Property="TextColor" Value="Azure" />
            </Style>
        </ResourceDictionary>
    </ContentPage.Resources>
    <StackLayout>
        <Label x:Name="id" Text="Loading...."/>
        <Label x:Name="name" Text="Loading...."/>
        <Label x:Name="age" Text="Loading...."/>
        <Label x:Name="gender" Text="Loading...."/>
        <Label x:Name="bdayYear" Text="Loading...."/>
        <Button Text="GO BACK NOW" Clicked="Button_Clicked"></Button>
    </StackLayout>
</ContentPage>